<canvas pySize="500x500" pySource="files/test/data/data.py" pyStyle="dark" pyBackground="(90,90,90)" pyLayout="11d2">
    <label pyWidth="40"> Shift </label>
    <label pyWidth="40"> Subheader </label>
    <label pyWidth="40"> left </label>
    <label pyWidth="40"> right </label>
    <label pyWidth="40"> Type </label>
    <label pyWidth="40"> {{ type_ }} </label>
    <label pyWidth="40"> Car </label>
    <label pyWidth="40"> {{ car }} </label>
    <list pyWidth="50">
        <list-item pyFor="name in crew">
            <label pyWidth="25"> Crew </label>
            <label pyWidth="75"> {{ name }}</label>
        </list-item>
    </list>
    <label pyWidth="40"> Start </label>
    <label pyWidth="40"> {{ start }} </label>
    <label pyWidth="40"> End </label>
    <label pyWidth="40"> {{ end }} </label>
</canvas>